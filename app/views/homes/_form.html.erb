<%= simple_form_for(@home) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :street_address %>
    <%= f.input :city %>
    <%= f.input :sate %>
    <%= f.input :zip %>
    <%= f.input :website %>
    <%= f.input :basic_description %>
    <%= f.input :details %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
